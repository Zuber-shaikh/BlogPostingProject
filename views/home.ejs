<h1>Home page</h1>
<% for (const post of posts) { %>
    <%= post.name %> <a href="<%= url %>/edit/<%= post.id %>" >Edit</a>
    <a href="<%= url %>/delete/<%= post.id %> "
       onclick="return confirm('Are you sure you want to delete this post?')" >
       Delete
    </a>
    <br>
<% } %>

<a href="<%= url %>/create/">Create New Post</a>